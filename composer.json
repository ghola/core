{
  "name": "phpservicebus/core",
  "description": "PHP Service Bus core library",
  "keywords": [
    "bus",
    "service",
    "service bus",
    "command bus",
    "cqrs"
  ],
  "homepage": "https://github.com/phpservicebus/core",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Alex Farcas",
      "email": "alex.farcas@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/ghola/php-rabbitmq-management-api.git"
    }
  ],
  "require": {
    "php": "^7.2,<7.5",
    "ext-mbstring": "*",
    "ext-json": "*",
    "ext-amqp": ">=1.7.0",
    "acclimate/container": "~2"
  },
  "require-dev": {
    "ext-pcntl": "*",
    "ext-posix": "*",
    "phpspec/phpspec": "6.1.1",
    "phpunit/phpunit": "8.5.2",
    "bossa/phpspec2-expect": "3.1.1",
    "empi89/php-amqp-stubs": "dev-master",
    "markup/rabbitmq-management-api": "dev-master#91509b6b2f53cd2dc1fdc4335421cad4de55e96e",
    "friends-of-phpspec/phpspec-code-coverage": "4.3.2"
  },
  "autoload": {
    "psr-4": {
      "PSB\\Core\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "acceptance\\PSB\\Core\\": "tests\\acceptance",
      "acceptancesupport\\PSB\\Core\\": "tests\\acceptancesupport",
      "spec\\PSB\\Core\\": "tests\\spec",
      "specsupport\\PSB\\Core\\": "tests\\specsupport"
    }
  },
  "scripts": {
    "test": [
      "phpspec run",
      "phpunit"
    ]
  },
  "config": {
    "bin-dir": "bin"
  },
  "extra": {
    "branch-alias": {
      "dev-devel": "2.x-dev"
    }
  }
}
